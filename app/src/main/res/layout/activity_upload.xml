<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_p3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.user.aaa.Upload">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="42dp"
        android:layout_marginTop="70dp"
        android:id="@+id/tva"
        android:text="a" />

    <TextView
        android:text="b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tva"
        android:layout_centerHorizontal="true"
        android:id="@+id/tvb" />

    <TextView
        android:text="c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvb"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="24dp"
        android:id="@+id/tvc" />

    <TextView
        android:text="d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tva"
        android:layout_alignStart="@+id/tva"
        android:layout_marginTop="56dp"
        android:id="@+id/tvd" />

    <TextView
        android:text="e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tvd"
        android:layout_alignStart="@+id/tvb"
        android:id="@+id/tve" />

    <TextView
        android:text="f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tve"
        android:layout_alignStart="@+id/tvc"
        android:id="@+id/tvf" />

    <Button
        android:text="上傳"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvg"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp"
        android:id="@+id/bt_ok" />

    <TextView
        android:text="g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:id="@+id/tvg"
        android:layout_below="@+id/tvd"
        android:layout_alignStart="@+id/tvd" />

    <TextView
        android:text="h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bt_ok"
        android:id="@+id/tvh"
        android:layout_alignStart="@+id/tve" />

    <Button
        android:text="回到主頁面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_ok"
        android:layout_alignStart="@+id/bt_ok"
        android:layout_marginTop="28dp"
        android:id="@+id/go_back" />

</RelativeLayout>
